@model SettingListModel

@{
    ViewBag.Title = T("Admin.Configuration.Settings.AllSettings").Value;
    Layout = "_SettingLayout";
    Assets.BodyAttributes.AppendCssClass("spa-layout");
}

@{
    var gridViewData = new ViewDataDictionary(this.ViewData);
    gridViewData["Parent"] = Model;
}

<partial name="_Grid.AllSettings" model="null" view-data="gridViewData" />