<Project Sdk="Microsoft.NET.Sdk.Razor">
  <PropertyGroup>
		<Product>Smartstore Twitter Authentication</Product>
		<OutputPath>..\..\Smartstore.Web\Modules\Smartstore.Twitter.Auth</OutputPath>
		<OutDir>$(OutputPath)</OutDir>
  </PropertyGroup>
	<ItemGroup>
		<Content Include="App_Data\**">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</Content>
	</ItemGroup>
	<ItemGroup>
	  <PackageReference Include="Microsoft.AspNetCore.Authentication.Twitter" Version="5.0.11" />
	</ItemGroup>
	<ItemGroup>
	  <Folder Include="wwwroot\" />
	</ItemGroup>
</Project>