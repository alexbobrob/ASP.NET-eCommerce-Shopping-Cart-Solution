  <LocaleResource Name="Plugins.Smartstore.PayPal" AppendRootKey="false">
    <Children>
      <LocaleResource Name="AdminInstruction">
        <Value>
          <![CDATA[
            Enter your data below to configure this plugin.
          ]]>
        </Value>
      </LocaleResource>      
      <LocaleResource Name="PaymentInfo">
        <Value>After completing the order you will be redirected to an external payment provider, where you can make the payment for your order.</Value>
      </LocaleResource>
      <LocaleResource Name="Agreement">
        <Value>I am aware that my data is transferred to the payment provider during the payment process.</Value>
      </LocaleResource>
      <LocaleResource Name="Agreement.Message">
        <Value>Please agree that your data may be transferred to the payment method provider during this payment process.</Value>
      </LocaleResource>
      <LocaleResource Name="UseSandbox">
        <Value>Use sandbox</Value>
      </LocaleResource>
      <LocaleResource Name="UseSandbox.Hint">
        <Value>Specifies whether the payments are executed in the sandbox of the payment provider.</Value>
      </LocaleResource>
      <LocaleResource Name="Account">
        <Value>Account</Value>
      </LocaleResource>
      <LocaleResource Name="Account.Hint">
        <Value>Enter the account that was sent to you by the payment provider.</Value>
      </LocaleResource>
			<LocaleResource Name="ClientId">
				<Value>Client ID</Value>
			</LocaleResource>
			<LocaleResource Name="ClientId.Hint">
				<Value>Enter the client ID that was sent to you by the payment provider.</Value>
			</LocaleResource>
      <LocaleResource Name="Secret">
        <Value>Password</Value>
      </LocaleResource>
      <LocaleResource Name="Secret.Hint">
        <Value>Enter the password that was sent to you by the payment provider.</Value>
      </LocaleResource>
      <LocaleResource Name="AdditionalFee">
        <Value>Additional fee</Value>
      </LocaleResource>
      <LocaleResource Name="AdditionalFee.Hint">
        <Value>Enter additional fee to charge your customers.</Value>
      </LocaleResource>
      <LocaleResource Name="AdditionalFeePercentage">
        <Value>Additional fee. Use percentage</Value>
      </LocaleResource>
      <LocaleResource Name="AdditionalFeePercentage.Hint">
        <Value>Specifies whether to apply a percentage additional fee to the order total. If not enabled, a fixed value is used.</Value>
      </LocaleResource>
			<LocaleResource Name="ShowButtonInMiniShoppingCart">
				<Value>Show button in mini shopping cart</Value>
			</LocaleResource>
			<LocaleResource Name="ShowButtonInMiniShoppingCart.Hint">
				<Value>Specifies whether to show the checkout button in the mini shopping cart.</Value>
			</LocaleResource>
			
			<LocaleResource Name="DisabledFundings">
				<Value>Disabled fundings</Value>
			</LocaleResource>
			<LocaleResource Name="DisabledFundings.Hint">
				<Value>
					The disabled funding sources for the transaction. Any funding sources passed are not displayed.
					Funding source eligibility is determined based on a variety of factors.
				</Value>
			</LocaleResource>
			<LocaleResource Name="EnabledFundings">
				<Value>Enabled fundings</Value>
			</LocaleResource>
			<LocaleResource Name="EnabledFundings.Hint">
				<Value>
					Funding source eligibility is determined based on a variety of factors.
					Enable funding can be used to ensure a funding source is rendered, if eligible.
				</Value>
			</LocaleResource>
			<LocaleResource Name="Intent">
				<Value>Transaction mode</Value>
			</LocaleResource>
			<LocaleResource Name="Intent.Hint">
				<Value>Specifies whether the payment is executed immediately or later through interaction of the merchant.</Value>
			</LocaleResource>
			<LocaleResource Name="Capture">
				<Value>Immediately debit</Value>
			</LocaleResource>
			<LocaleResource Name="Auth">
				<Value>Authorize immediately, debit later</Value>
			</LocaleResource>
			<LocaleResource Name="BillingPlanName">
				<Value>Billing plan {1}</Value>
			</LocaleResource>
    </Children>
  </LocaleResource>
	
